<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.inphynous.futuresupport8x">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/future_supportt"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/future_supportt_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".Installment_history"></activity>
        <activity android:name=".Update_member_installment" />
        <activity
            android:name=".Add_loan_record"
            android:parentActivityName=".Loan_status" />
        <activity
            android:name=".Loan_status"
            android:parentActivityName=".AccountActivity" />

        <uses-library
            android:name="org.apache.http.legacy"
            android:required="false" />

        <activity
            android:name=".update_acc_detail"
            android:parentActivityName=".AccountActivity" />
        <activity android:name=".admin" />
        <activity
            android:name=".Add_Data_Activity"
            android:parentActivityName=".HomeActivity" />
        <activity
            android:name=".unique"
            android:parentActivityName=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Detailp"
            android:parentActivityName=".HomeActivity" />
        <activity
            android:name=".update_installment_record"
            android:parentActivityName=".installment" />
        <activity
            android:name=".installment"
            android:parentActivityName=".HomeActivity" />
        <activity
            android:name=".AccountActivity"
            android:parentActivityName=".HomeActivity" />
        <activity
            android:name=".signupactivity"
            android:parentActivityName=".LoginActivity" />
        <activity
            android:name=".signup"
            android:parentActivityName=".LoginActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".HomeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>